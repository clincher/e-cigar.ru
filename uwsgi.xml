<uwsgi>
    <socket>/web/run/ecigar.sock</socket>
    <process>1</process>
    <master/>
    <uid>web</uid>
    <gid>web</gid>
    <pidfile>/web/run/ecigar.pid</pidfile>
    <env>DJANGO_SETTINGS_MODULE=settings</env>
    <module>django.core.handlers.wsgi:WSGIHandler()</module>
    <chdir>/web/ecigar</chdir>
    <virtualenv>/home/web/.virtualenvs/ecigar</virtualenv>
    <daemonize>/web/log/ecigar-uwsgi.log</daemonize>
</uwsgi>